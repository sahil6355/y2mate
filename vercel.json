{
    "version": 2,
    "builds": [
      {
        "src": "src/**/*",
        "use": "@vercel/static"
      },
      {
        "src": "api/**/*",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/download",
        "dest": "/api/download.js"
      }
    ]
  }
  